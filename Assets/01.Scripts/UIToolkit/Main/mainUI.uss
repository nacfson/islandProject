.infoUI {
    bottom: auto;
    width: 60%;
    height: 30%;
    background-color: rgb(255, 255, 255);
    top: 60%;
    left: 20%;
    border-top-left-radius: 50px;
    border-bottom-left-radius: 50px;
    border-top-right-radius: 50px;
    border-bottom-right-radius: 50px;
    transition-property: all;
    transition-duration: 0.5s;
    scale: 0 0;
}

.infoUI.active {
    scale: 1 1;
    flex-shrink: 0;
}

.nameUI {
    width: 20%;
    height: 20%;
    bottom: 40%;
    background-color: rgb(255, 123, 0);
    border-top-left-radius: 14px;
    border-bottom-left-radius: 14px;
    border-top-right-radius: 14px;
    border-bottom-right-radius: 14px;
    rotate: -13deg;
    left: -5%;
}

.contentLabel {
    align-items: center;
    justify-content: center;
    -unity-text-align: middle-center;
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    top: 35%;
    font-size: 76px;
    -unity-font-style: normal;
}

.inventoryUI {
    width: 70%;
    height: 50%;
    background-color: rgb(176, 176, 176);
    flex-grow: 1;
    flex-shrink: 0;
    top: 10%;
    position: absolute;
    left: 15%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-top-right-radius: 80px;
    border-bottom-right-radius: 80px;
    rotate: 0;
    flex-direction: row;
    flex-wrap: wrap;
    padding-left: 3px;
    padding-right: 3px;
    padding-top: 3px;
    padding-bottom: 3px;
    justify-content: center;
    align-items: center;
    align-self: center;
    scale: 0 0;
    transition-duration: 0.5s;
    background-image: none;
}

.inventorySlot {
    width: 10%;
    height: 30%;
    background-color: rgba(111, 111, 111, 0);
    border-top-left-radius: 54px;
    border-bottom-left-radius: 54px;
    border-top-right-radius: 54px;
    border-bottom-right-radius: 54px;
    background-image: url('project://database/Assets/02.Assets/Masalimov%20Ilnur/Professional%20Pixel%20RPG%20Icon%20Pack/Sprites/Panel.png?fileID=2800000&guid=cceae44e769a40244ad9642c0f71992a&type=3#Panel');
}

.inventorySlot.image {
    flex-grow: 1;
    background-color: rgba(255, 255, 255, 0.24);
    flex-shrink: 0;
    width: 100%;
    height: 100%;
    bottom: 0;
    right: 0;
    background-image: url('project://database/Assets/02.Assets/Models/Topdown%20Island%20Pack/Materials_textures/Smoke.tga?fileID=2800000&guid=ff655d3619e794b49802b22a40ba671e&type=3#Smoke');
}

.inventorySlot.count {
    bottom: -30px;
    flex-shrink: 1;
    position: absolute;
    right: 38px;
    background-color: rgba(0, 0, 0, 0);
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    font-size: 40px;
    -unity-font-style: bold;
}

.inventoryUI.active {
    scale: 1 1;
}

.fadeUI {
    background-color: rgb(0, 0, 0);
    width: 100%;
    height: 100%;
    flex-shrink: 0;
    scale: 1 1;
}

.fadeUI.active {
}

.shopUI {
    scale: 0 0;
    flex-shrink: 0;
    background-color: rgba(255, 255, 255, 0);
    width: 87%;
    height: 100%;
    flex-grow: 1;
    position: absolute;
    transition-duration: 0.5s;
    top: -127px;
    right: -11px;
}

.shopUI.active {
    scale: 1 1;
}

.shopView {
    right: 5%;
    width: 40%;
    height: 70%;
    background-color: rgb(255, 129, 129);
    position: absolute;
    top: 3%;
    border-top-left-radius: 28px;
    border-bottom-left-radius: 28px;
    border-top-right-radius: 28px;
    border-bottom-right-radius: 28px;
    padding-left: 26px;
    padding-right: 26px;
    padding-top: 26px;
    padding-bottom: 26px;
}

.item {
    height: 54px;
    flex-grow: 1;
    flex-shrink: 0;
    background-color: rgb(254, 255, 109);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    flex-direction: row;
    margin-left: 9px;
    margin-right: 9px;
    margin-top: 9px;
    margin-bottom: 9px;
}

.item > .image {
    flex-shrink: 0;
    width: 8%;
    background-color: rgb(255, 255, 255);
    border-top-left-radius: 8px;
    border-bottom-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

.item.select {
    background-color: rgb(245, 179, 179);
}

.item > .label {
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    -unity-font-style: bold;
    font-size: 34px;
    -unity-text-align: middle-left;
}

.shopUI > .button {
    width: 10%;
    height: 5%;
    background-color: rgb(141, 255, 131);
    position: absolute;
    bottom: 5%;
    right: 40%;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    font-size: 26px;
    transition-duration: 0.5s;
}

.shopUI > .button:hover {
    scale: 0.8 0.8;
}

.selectUI {
    scale: 0 0;
    flex-shrink: 0;
    flex-grow: 1;
    position: absolute;
    width: 15%;
    height: 32%;
    background-color: rgb(122, 147, 109);
    border-top-left-radius: 39px;
    border-bottom-left-radius: 39px;
    border-top-right-radius: 39px;
    border-bottom-right-radius: 39px;
    padding-top: 34px;
}

.selectUI.active {
    scale: 1 1;
    transition-duration: 0.5s;
}

.selectUI > .button {
    border-top-left-radius: 8px;
    border-bottom-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    background-color: rgb(204, 203, 84);
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    font-size: 47px;
    margin-left: 11px;
    margin-right: 11px;
    margin-top: 11px;
    margin-bottom: 11px;
}

.moneyUI {
    scale: 0 0;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 8px;
    border-right-width: 8px;
    border-top-width: 8px;
    border-bottom-width: 8px;
    flex-shrink: 0;
    flex-grow: 1;
    width: 13%;
    height: 10%;
    position: absolute;
    top: 0;
    background-color: rgb(226, 189, 95);
    border-top-left-radius: 45px;
    border-bottom-left-radius: 45px;
    border-top-right-radius: 45px;
    border-bottom-right-radius: 45px;
    margin-left: 61px;
    margin-right: 61px;
    margin-top: 61px;
    margin-bottom: 61px;
}

.moneyUI.active {
    scale: 1 1;
}

.moneyUI > .image {
    background-color: rgba(255, 20, 20, 0);
    width: 40%;
    height: 85%;
    top: -64%;
    left: 30%;
    background-image: url('project://database/Assets/02.Assets/0.Sprite/Money.png?fileID=2800000&guid=4d1f6c2141fec134a949e38e507c068f&type=3#Money');
    border-left-width: 8px;
    border-right-width: 8px;
    border-top-width: 8px;
    border-bottom-width: 8px;
    border-left-color: rgba(243, 243, 243, 0);
    border-right-color: rgba(243, 243, 243, 0);
    border-top-color: rgba(243, 243, 243, 0);
    border-bottom-color: rgba(243, 243, 243, 0);
    flex-shrink: 0;
    flex-grow: 1;
}

.moneyUI > .text {
    align-items: center;
    justify-content: center;
    -unity-text-align: middle-center;
    text-overflow: clip;
    -unity-font-definition: url('project://database/Assets/08.Fonts/Asia%EC%8B%A0%EB%94%94%EB%82%98%EB%A3%A8M%20SDF%201.asset?fileID=11400000&guid=68bf536b04e874c4d859fa5ed5b46fe1&type=2#Asia신디나루M SDF 1');
    -unity-font-style: bold;
    top: -50%;
    font-size: 30px;
    color: rgb(255, 255, 255);
}
